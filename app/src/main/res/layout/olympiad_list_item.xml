<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/card_view"
    android:layout_width="match_parent"
    android:layout_height="108dp"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_centerVertical="true"
    android:layout_gravity="center"
    android:layout_marginBottom="4dp"
    android:layout_marginTop="8dp"
    android:elevation="1dp"
    app:cardElevation="2dp"
    card_view:cardCornerRadius="4dp"
    tools:layout_conversion_absoluteX="0dp"
    tools:layout_conversion_absoluteY="0dp">

    <!--This View serves as a visual divider between list items-->

    <TextView
        android:id="@+id/formula_two"
        android:layout_width="match_parent"
        android:layout_height="107dp"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="93dp"
        android:fontFamily="sans-serif-condensed"
        android:gravity="center"
        android:lineSpacingExtra="8sp"
        android:textAlignment="center"
        android:textColor="@drawable/color_selector"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_conversion_absoluteHeight="60dp"
        tools:layout_conversion_absoluteWidth="324dp"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="0dp"></TextView>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="97dp"
        android:layout_height="92dp"
        android:layout_marginTop="7.5dp"
        app:srcCompat="@drawable/intern" />
</android.support.v7.widget.CardView>
